<div class="container">
  <ul>
    <% @bookings.where(user == current_user).each do |booking| %>
    <div class="booking_card">
      <li>
        <%= link_to "see details", booking_path(booking) %>
        <%= booking.doppelganger.name %>,
        <%= booking.address %>,
        <%= booking.start_date %>,
        <%= booking.end_date %>
      </li>
    </div>
    <% end %>
  </ul>
</div>
