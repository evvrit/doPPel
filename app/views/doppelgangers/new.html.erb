  <div class="container">
    <p>create a new doppelganger </p>
    <div class="form_doppelganger">
      <%= simple_form_for @doppelganger do |f| %>
        <%= f.input :name %>
        <%= f.input :age %>
        <%= f.input :address, label: "Street Address. *in Montreal, Ottawa, or Toronto." %>
        <%= f.input :bio, length: { in: 10..280 } %>
        <%= f.input :rate %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
