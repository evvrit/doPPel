  <div class="container">
    <p>create a new doppelganger </p>
    <div class="form_doppelganger">
      <%= simple_form_for @doppelganger do |f| %>
        <%= f.input :name %>
        <%= f.input :age %>
        <%= f.input :address, label: "Street Address" %>
        <%= f.input :bio %>
        <%= f.input :rate %>
        <%= f.input :photo, as: :file %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
