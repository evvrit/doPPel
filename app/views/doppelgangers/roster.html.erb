<h1>Here is your roster</h1>
  <br>
<div class="container">
  <ul>
    <% @roster.each do |doppelganger| %>
      <div class="card">
        <img src="https://ca.slack-edge.com/T02NE0241-U044LJM9RL5-ce7c2bd0f2fc-512" style="width: 200px" class="card-img-top">
        </div class="card-body">
          <h5><%= link_to doppelganger.name, doppelganger_path(doppelganger) %></h5>
              <p><%= doppelganger.age %></p>
              <p><%= doppelganger.location %></p>
              <p><%= doppelganger.rate %></p>
              <p><%= doppelganger.bio %></p>
        </div>
        <% end %>
      </div>
  </ul>
</div>
